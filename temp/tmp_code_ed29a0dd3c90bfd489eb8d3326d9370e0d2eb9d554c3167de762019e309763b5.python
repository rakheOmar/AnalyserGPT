# Import necessary libraries
import pandas as pd
import matplotlib.pyplot as plt

# Load the CSV file into a DataFrame
df = pd.read_csv('titanic.csv')

# Count the number of survivors and deaths
survivors_count = df['Survived'].value_counts()[0]
deaths_count = df['Survived'].value_counts()[1]

# Create a pie chart to visualize the result
plt.figure(figsize=(8, 8))
plt.pie([survivors_count, deaths_count], labels=['Survived', 'Died'], autopct='%1.1f%%')
plt.title('Survival Rate on Titanic')
plt.savefig('output.png')

print(f'There were {survivors_count} survivors and {deaths_count} deaths.')
